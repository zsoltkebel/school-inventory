<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root type="javafx.scene.control.ListCell" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
         prefWidth="500">
    <graphic>
        <GridPane fx:id="root" hgap="10" vgap="5">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>

            <Label fx:id="nameLabel" text="Name" maxWidth="Infinity"
                   stylesheets="/styles/emphasis.css"
                   GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <Label fx:id="descriptionLabel" text="Description" maxWidth="Infinity"
                   GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <Label fx:id="categoryLabel" alignment="CENTER_RIGHT" text="Category"
                   GridPane.rowIndex="0" GridPane.columnIndex="1" GridPane.rowSpan="2"/>
            <Button fx:id="reserveButton" text="Reserve" maxWidth="Infinity" maxHeight="Infinity"
                    GridPane.rowIndex="0" GridPane.columnIndex="2" GridPane.rowSpan="2"/>
        </GridPane>
    </graphic>
</fx:root>
