<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<HBox fx:id="itemPane" spacing="10" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      stylesheets="/styles/panes.css">
    <padding>
        <Insets topRightBottomLeft="5"/>
    </padding>
    <VBox HBox.hgrow="ALWAYS">
        <Label fx:id="nameLabel" text="Label"
               stylesheets="/styles/emphasis.css"/>
        <Label fx:id="descriptionLabel" text="Label" maxWidth="140" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </VBox>
    <AnchorPane minHeight="0.0" minWidth="0.0">
        <Label fx:id="categoryLabel" alignment="CENTER_RIGHT" text="Label" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </AnchorPane>
</HBox>
