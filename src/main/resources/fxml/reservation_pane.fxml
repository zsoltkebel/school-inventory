<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="reservationPane" spacing="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" stylesheets="/styles/panes.css">
    <padding><Insets topRightBottomLeft="8" /></padding>
    <HBox alignment="CENTER_LEFT" spacing="10">
        <Label fx:id="nameLabel" maxWidth="Infinity" text="Name" HBox.hgrow="ALWAYS" />
        <Button fx:id="returnButton" text="Returned" />
    </HBox>
    <Label fx:id="dateLabel" text="2020.01.04." />

</VBox>
